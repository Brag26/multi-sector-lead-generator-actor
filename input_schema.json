{
  "title": "Multi-LLM AI-Powered Lead Generator",
  "description": "Generate business leads using AI to intelligently find relevant businesses.",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "sector": {
      "title": "Sector",
      "type": "string",
      "editor": "select",
      "enum": [
        "Healthcare",
        "Real Estate",
        "Manufacturing",
        "IT & Technology",
        "Education & Training",
        "Legal Services",
        "Financial Services",
        "Hospitality & Tourism",
        "Retail & E-commerce",
        "Food & Beverage",
        "Construction",
        "Automotive",
        "Marketing & Advertising",
        "Consulting",
        "Logistics & Transportation",
        "Beauty & Wellness",
        "Entertainment & Media",
        "Agriculture",
        "Energy & Utilities",
        "Telecommunications",
        "Insurance",
        "Professional Services",
        "Non-Profit & NGO",
        "Sports & Fitness"
      ]
    },

    "country": {
      "title": "Country (Optional)",
      "description": "Enter any country (e.g., India, Australia, USA). Leave empty for global search.",
      "type": "string",
      "editor": "textfield"
    },

    "state": {
      "title": "State / Province (Optional)",
      "type": "string",
      "editor": "textfield"
    },

    "city": {
      "title": "City / Suburb (Optional)",
      "type": "string",
      "editor": "textfield"
    },

    "postcode": {
      "title": "Postcode / ZIP Code (Optional)",
      "type": "string",
      "editor": "textfield"
    },

    "keyword": {
      "title": "Keyword (Optional)",
      "type": "string",
      "editor": "textfield"
    },

    "maxResults": {
      "title": "Maximum Results",
      "type": "integer",
      "default": 10,
      "minimum": 1,
      "maximum": 100
    }
  },
  "required": ["sector"]
}
